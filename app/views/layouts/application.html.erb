<!DOCTYPE html>
<html>
  <head>
    <title>Borboleta Coding Challenge</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="ui container">
    <%= render partial: "shared/nav_bar" %>
      <% flash.each do |key, value| %>
        <%= render partial: "shared/flash", locals: {message_type: key, message: value} %>
      <% end %>
    <%= yield %>
  </body>
</html>
